.container{
  padding: 20px 20px 60px 20px;
}
.content-header{
   display: flex;
      margin-bottom: 32px;
      width: 100%;
}
.img{
   width: 80px;
        height: 80px;
        margin-right: 12px;
}
.img image{
   display: block;
          width: 100%;
          height: 100%;
}
.info{
   flex: 1;
        font-size: 0;
}
.info .money{
 font-size: 20px;
          line-height: 16px;
          color: rgb(173, 37, 38);
          font-weight: 600;
          
}
.money .typeI{
   font-style: normal;
            font-weight: normal;
            font-size: 12px;
            display: inline-block;
}
.info .title{
  font-size: 16px;
          line-height: 20px;
          color: rgb(85,85,85);
          font-weight: 700;
          margin: 4px 0 6px 0;
          overflow: hidden;
          text-overflow: ellipsis;
          display: -webkit-box;
          -webkit-line-clamp: 2;
          -webkit-box-orient: vertical;
          white-space: pre-wrap;
          word-break: break-all;
}
.info .type{
   font-size: 12px;
          line-height: 12px;
          color: rgb(179,179,179);
          font-weight: 700;
}
.itemList{
  width: 100%;
}
.item{
        font-size: 13px;
        color: rgb(85,85,85);
        font-weight: 700;
        padding: 6px 14px;
        border-radius: 40px;
        background-color: rgb(247,247,247);
        margin-right: 16px;
        margin-bottom: 18px;
        display: inline-block;
        
      }
      .itemNull{
        color: rgb(166,166,166);
      }
      .itemActive{
        background-color: rgb(173, 37, 38);
        color: #fff;
      }
.ticketNum{
  overflow: hidden;
      margin-top: 26px;
     width: 100%;
}
.num{
        font-size: 14px;
        color: rgb(166,166,166);
        font-weight: 700;
        float: left;
        line-height: 20px;
      }
.compute{
  float: right;
}
.compute view{
   display: inline-block;
          float: left;
}
.reduce{

          width: 20px;
          height: 20px;
          
        float: left;
        }
        .number{
          font-size: 14px;
          font-weight: 600;
          padding: 0 12px;
        }
        .add{
          width: 20px;
          height: 20px;
         float: right;
        }
.footer{
      position: fixed;
      bottom: 0;
      left: 0;
      right: 0;
      height: 50px;
      background-color: rgb(173, 37, 38);
      font-size: 18px;
      font-weight: 700;
      line-height: 50px;
      text-align: center;
      color: #fff;
    }